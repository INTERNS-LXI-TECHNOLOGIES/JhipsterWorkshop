
entity Vegetable {
	id Long,
    name String,
    price String,
    stock String,
    minOrderQuantity String,
    base64Image String,
    image Blob

    
}

entity Category {
id Long,
	categoryType String
}


// Use Data Transfer Objects (DTO// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

// Set an angular suffix
// angularSuffix * with mySuffix
relationship OneToMany{

Category{vegetables} to Vegetable{category}

}

